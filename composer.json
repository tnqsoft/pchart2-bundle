            "email": "tuanquynh0508@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.5.9",
        "symfony/symfony": "3.3.*"
    },
    "minimum-stability": "dev",
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    },
	"autoload": {
        "psr-4": {
            "Pchart2Bundle\\": "src/"
        }
    }
